{
  "name": "heroes-app",
  "private": true,
  "version": "1.0.0",
  "description": "Una aplicación web inmersiva inspirada en Heroes of the Storm para explorar el universo de héroes de Blizzard Entertainment",
  "type": "module",
  "homepage": "https://adravilag.github.io/heroes-app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adravilag/heroes-app.git"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "heroes",
    "blizzard",
    "gaming",
    "spa",
    "i18n"
  ],
  "author": "Adravilag",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Adravilag/heroes-app/issues"
